#include <iostream>
using namespace std;

int main (){
  int number;
  cin >> number;

  char *slova = new char[number];

  for (int i = 0; i < number; i++){
    cin >> slova[i];
  }

  for (int i = 0; i < number; i++){
    slova[i] -= 32;
  }

  for (int i = 0; i < number; i++){
    cout << slova[i] << " ";
  }
}
